<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.SafeStudy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Hackathon2021">
        <activity android:name="com.example.SafeStudy.MapActivity"></activity>
        <activity android:name="com.example.SafeStudy.ChatActivity" />
        <activity android:name="com.example.SafeStudy.CheckInActivity" />
        <activity android:name="com.example.SafeStudy.CreateActivity" />
        <activity android:name="com.example.SafeStudy.SearchActivity" />
        <activity android:name="com.example.SafeStudy.ProfileActivity" />
        <activity android:name="com.example.SafeStudy.SecondActivity" />
        <activity android:name="com.example.SafeStudy.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>